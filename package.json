    "target": "ES6",
    "module": "commonjs",
    "strict": false,
    "type": "module",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "scripts": {
