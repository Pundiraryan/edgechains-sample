  ],
  "dependencies": {
    "@arakoodev/edgechains.js": "^0.30.1",
    "@arakoodev/jsonnet": "^0.25.0",
    "ace-builds": "^1.36.5",
    "body-parser": "^1.20.3",
    "express": "^4.21.2",
    "groq-sdk": "^0.9.1",
    "react-ace": "^13.0.0",
    "sync-rpc": "^1.3.6"
  }
}
